{
    "name": "inhouse_analyzer",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "build": "concurrently --kill-others-on-fail \"yarn server\" node server.js"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "canvasjs": "^1.8.1",
        "express": "^4.16.4",
        "react-scripts": "^2.1.8"
    },
    "devDependencies": {
        "concurrently": "^4.0.1"
    }
}